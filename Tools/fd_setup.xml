<?xml version="1.0" encoding="UTF-8"?> 
<OpenSimDocument Version="40000"> 
<ForwardTool name="arm26"> 
 
  <!-- OpenSim Model --> 
  <model_file> C:\Users\Jaxton\OneDrive\Project\arm26.osim </model_file> 
  <force_set_files> reserve_actuators.xml </force_set_files> 
  <replace_force_set> false </replace_force_set>
 
  <!-- Times over which to run forward --> 
  <initial_time> 0.03 </initial_time> 
  <final_time> 0.49 </final_time> 
 
  <!-- Integrator Settings --> 
  <maximum_number_of_integrator_steps> 20000 </maximum_number_of_integrator_steps> 
  <maximum_integrator_step_size>1</maximum_integrator_step_size>
  <minimum_integrator_step_size>1e-08</minimum_integrator_step_size>
  <integrator_error_tolerance> 5e-06 </integrator_error_tolerance> 
  <use_specified_dt> false </use_specified_dt> 
 
  <!--Analyses and Results --> 
  <results_directory> FD_Results </results_directory> 
  <output_precision> 20 </output_precision> 
  <AnalysisSet name="Analyses"> 
    <objects> 
        <Kinematics name="Kinematics">
            <!--Flag (true or false) specifying whether on. True by default.-->
            <on>false</on>
            <!--Specifies how often to store results during a simulation. More specifically, the interval (a positive integer) specifies how many successful integration steps should be taken before results are recorded again.-->
            <step_interval>1</step_interval>
            <!--Flag (true or false) indicating whether the results are in degrees or not.-->
            <in_degrees>true</in_degrees>
        </Kinematics>
        <Actuation name="Actuation">
            <!--Flag (true or false) specifying whether on. True by default.-->
            <on>false</on>
            <!--Specifies how often to store results during a simulation. More specifically, the interval (a positive integer) specifies how many successful integration steps should be taken before results are recorded again.-->
            <step_interval>1</step_interval>
            <!--Flag (true or false) indicating whether the results are in degrees or not.-->
            <in_degrees>true</in_degrees>
        </Actuation>
        <BodyKinematics name="BodyKinematics">
            <!--Flag (true or false) specifying whether on. True by default.-->
            <on>false</on>
            <!--Specifies how often to store results during a simulation. More specifically, the interval (a positive integer) specifies how many successful integration steps should be taken before results are recorded again.-->
            <step_interval>1</step_interval>
            <!--Flag (true or false) indicating whether the results are in degrees or not.-->
            <in_degrees>true</in_degrees>
        </BodyKinematics>
    </objects> 
  </AnalysisSet> 
 
 
  <!-- Initial states and controls --> 
  <states_file> CMC_Results/cmc_output_states.sto </states_file> 
  <ControllerSet name="Controllers">
    <objects>
      <ControlSetController name="controlsetcontroller">
        <actuator_list> </actuator_list>
        <enable_controller> ALL </enable_controller>
        <controls_file> CMC_Results/cmc_output_controls.sto </controls_file>
      </ControlSetController>
    </objects>
  </ControllerSet>
  
  <!-- External Loads --> 
  <external_loads_file />
  
</ForwardTool> 
</OpenSimDocument>